(jbuild_version 1)

(executables
 ((names      (dims ini))
  (modules    (Dims Ini))
  (libraries  (tyre))
  (modes (byte))
  (link_flags(
    -I +compiler-libs
    ocamlcommon.cma 
    ocamlbytecomp.cma
    ocamltoplevel.cma
    metalib.cma 
    berstart.cmo
  ))
))

(alias
 ((name runtest)
  (deps (dims.exe))
  (action (run ${exe:dims.exe}))
))

(alias
 ((name runtest)
  (deps (ini.exe))
  (action (run ${exe:ini.exe}))
))
